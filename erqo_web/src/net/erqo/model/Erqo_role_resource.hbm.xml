<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="net.erqo.model">
    <class name="Erqo_role_resource" table="ERQO_ROLE_RESOURCE" lazy="true" dynamic-update="true" dynamic-insert="true">
	  	<cache usage="nonstrict-read-write"/>
        <comment>role resource table</comment>
		<id name="id" column="ID">
			<generator class="native" />
		</id>
		<property name="role_id" column="role_id" />
		<property name="resource_id" column="resource_id" />
    </class>
</hibernate-mapping>
